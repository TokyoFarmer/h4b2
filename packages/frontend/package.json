{
  "name": "@h4bff/frontend",
  "version": "1.0.2",
  "description": "H4BFF, the frontend: mobx router.",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build/**/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "scripts": {
    "prepublish": "yarn build && yarn test",
    "build": "tsc -p tsconfig.json",
    "watch": "tsc -w",
    "test": "jest"
  },
  "dependencies": {
    "@h4bff/core": "*",
    "lodash": "^4.17.5",
    "mobx": "^4.6.0",
    "mobx-react": "^5.4.2",
    "query-string": "^6.2.0",
    "react": "^16.7.7",
    "react-dom": "^16.6.3",
    "classnames": "^2.2.6",
    "history": "^4.7.2",
    "path-to-regexp": "^3.0.0",
    "@types/react": "^16.7.13",
    "@types/history": "^4.7.2"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.6",
    "@types/lodash": "^4.14.63",
    "@types/query-string": "^6.2.0",
    "@types/react-dom": "^16.0.10",
    "@types/react-test-renderer": "^16.8.1",
    "react-test-renderer": "^16.8.4"
  },
  "jest": {
    "verbose": false,
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json",
        "isolatedModules": true
      }
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": ".*\\.spec\\.tsx?",
    "collectCoverageFrom": [
      "src/**/*.tsx?"
    ],
    "coverageDirectory": "coverage"
  }
}
